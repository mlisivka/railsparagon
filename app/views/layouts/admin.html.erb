<!DOCTYPE html>
<html>
  <head>
		
    <title>ParagonRails</title>
    <%= stylesheet_link_tag    'admin', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
		<meta name="google-site-verification" content="KyfySNzoGJK-uKC5P_7X6_qk32BcsW2qdm1-bP3kRYo" />
  </head>
  <body>
		
		<nav class="navbar navbar-default navbar-static-top">
		  <div class="container-fluid">
		    <div class="navbar-header">
		      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
		        <span class="icon-bar"></span>
		        <span class="icon-bar"></span>
		        <span class="icon-bar"></span>
		      </button>
		      <div class="navbar-brand navbar-brand-name">RailsParagon</div>
		    </div>
		    <div class="collapse navbar-collapse" id="myNavbar">
		      <ul class="nav navbar-nav">
		        <li><%= link_to "Dashboard", admin_root_path %></li>
		        <li><%= link_to "User", admin_users_path %></li>
						<li><%= link_to "Tournaments", admin_tournaments_path %></li>
						<li><%= link_to "Posts", admin_posts_path %></li>
						<li><%= link_to "Teams", admin_teams_path %></li>
		      </ul>
		    </div>
		  </div>
		</nav>
		
    <%= yield %>
		
  </body>
</html>
