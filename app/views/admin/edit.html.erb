<div class="jumbotron">
  <div class="container-fluid">
    <div class="titlebar_left">
      <h2><%= controller.controller_name.humanize %></h2>
		</div>
  </div>
</div>
<div class="container-fluid">
  <div class="content">
    <div class="col-sm-12">
      <div class="well">
				
				<%= form_for @record do |i| %>
				<% @column.each do |col| %>
				<div class="form-group row">
						<%= i.label col, col.humanize , class: "col-sm-2 form-control-label" %>
				    <div class="col-sm-10">
							<%= i.text_field col, autofocus: true, class: "form-control" %>
				    </div>
				  </div>
				<% end %>
				
			    <div class="form-group row">
			      <div class="col-sm-offset-2 col-sm-10">
							<%= i.submit "Save", class: "btn btn-default" %>
							<%= link_to "Cancel", url_for(action: :index), class: "btn btn-default" %>
			      </div>
			    </div>
		    <% end %>
      </div>
    </div>
  </div>
</div>